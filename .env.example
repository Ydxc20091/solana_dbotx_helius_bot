# DBotX API key
DBOTX_API_KEY=your_dbotx_key
# Kline endpoint template
DBOTX_KLINE_URL="https://api-data-v1.dbotx.com/kline/chart?chain={chain}&pair={pair}&interval={interval}&end={end}"
# Trading pair chain and address
DBOTX_CHAIN=solana
DBOTX_PAIR=PAIR_ADDRESS
# RPC endpoint
RPC_URL=https://mainnet.helius-rpc.com/?api-key=your_helius_api_key
# Wallet public key and secure code
PUBLIC_KEY=your_public_key
SECURE_CODE=your_secure_code
# Base and quote token symbols
BASE=TOKEN
QUOTE=SOL
# Order size in SOL
ORDER_SIZE_SOL=0.5
